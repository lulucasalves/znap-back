{
  "version": 2,
  "builds": [
    {
      "src": "./dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "categories",
      "methods": ["GET"],
      "dest": "./dist/server.js"
    },
    {
      "src": "category",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "./dist/server.js"
    }
  ]
}
